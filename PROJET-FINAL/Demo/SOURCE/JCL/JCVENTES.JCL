//API1COMP JOB (ACCT#),'ALEXIS',CLASS=A,MSGCLASS=H,REGION=4M,
//             MSGLEVEL=(1,1),NOTIFY=&SYSUID,COND=(4,LT),TIME=(,30)
//*
//PROCLIB  JCLLIB ORDER=SDJ.FORM.PROCLIB
//         SET SYSUID=API1,
//             NOMPGM=VENTES
//* COMPIL VENTES
//APPROC   EXEC COMPDB2
//STEPDB2.SYSLIB   DD DSN=&SYSUID..SOURCE.DCLGEN,DISP=SHR
//STEPDB2.SYSIN    DD DSN=&SYSUID..SOURCE.COBOL(&NOMPGM),DISP=SHR
//STEPDB2.DBRMLIB  DD DSN=&SYSUID..SOURCE.DBRMLIB(&NOMPGM),DISP=SHR
//STEPLNK.SYSLMOD  DD DSN=&SYSUID..SOURCE.PGMLIB(&NOMPGM),DISP=SHR
//STEPLNK.SYSLIN   DD DSN=&SYSUID..SOURCE.PGMLIB(CONVDATE),DISP=SHR
//STEPCOB.SYSLIB   DD DSN=&SYSUID..SOURCE.COPY,DISP=SHR
//*
//BDPART2  EXEC  PGM=IKJEFT01,COND=(4,LT)
//DBRMLIB    DD  DSN=&SYSUID..SOURCE.DBRMLIB,DISP=SHR
//SYSTSPRT   DD  SYSOUT=*,OUTLIM=25000
//SYSTSIN    DD  *
 DSN SYSTEM (DSN1)
  BIND PLAN      (VENTES) -
       QUALIFIER (API1)    -
       ACTION    (REPLACE) -
       MEMBER    (VENTES) -
       VALIDATE  (BIND)    -
       ISOLATION (CS)      -
       ACQUIRE   (USE)     -
       RELEASE   (COMMIT)  -
       EXPLAIN   (NO)
